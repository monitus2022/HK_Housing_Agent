app_name=HK Housing Agent Backend
app_description=Backend service for HK Housing Agent application
app_version=1.0.0
host=0.0.0.0
port=8000

openrouter_api_url=https://openrouter.ai/api/v1
openrouter_api_key=
openrouter_model=meta-llama/llama-3.3-8b-instruct:free

llm_max_tokens=1024

# Cloud Storage Configuration (works for both Cloudflare R2 and AWS S3)
cloud_storage_service_type=cloudflare  # Options: 'cloudflare' or 'aws'
cloud_storage_access_key_id= 
cloud_storage_secret_access_key=
cloud_storage_bucket_name=hk-housing-datahub
cloud_storage_bucket_region=auto
cloudflare_endpoint_url=https://{cloudflare_account_id}.r2.cloudflarestorage.com
aws_endpoint_url=https://s3.{bucket_region}.amazonaws.com

# Cloudflare R2 specific (leave empty for AWS S3)
cloudflare_account_id=
